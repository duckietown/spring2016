<launch>
	<arg name="veh"/>
	<include file="$(find duckietown)/machines"/>
	
	<group ns="$(arg veh)">
		<node machine="$(arg veh)" name="virtual_mirror_joewl_node" pkg="virtual_mirror_joewl" type="virtual_mirror_joewl_node.py" output="screen">

			<!-- Remapping topics -->
			<remap from="~image_in" to="camera_node/image/compressed"/>
		</node>
	</group>
</launch>
