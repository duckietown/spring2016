<launch>
	<arg name="veh"/>
	<arg name="log_in"/>
	<arg name="log_out"/>
	
	<include file="$(find image_average-joe-wl)/launch/image_average_joewl_node.launch">
		<arg name="veh" value="$(arg veh)"/>
	</include>

	<node pkg="rosbag" type="play" name="rosbag_play" args="$(arg log_in)"/>
	<node pkg="rosbag" type="record" name="rosbag_record" args="-a  -O $(arg log_out)"/>
</launch>
